{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-icons": "^3.1.0",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/stylesheets/base.scss ./src/stylesheets/index.css --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/stylesheets/base.scss  ./src/stylesheets/index.css --watch --recursive --output-style compressed",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
